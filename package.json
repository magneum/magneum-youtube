{
  "name": "yt-dlp",
  "type": "module",
  "types": "Types",
  "version": "0.0.7",
  "main": "app/index.js",
  "description": "yt-dlp - Python+ Javascript Youtube Audio Video Scrapper And Downloader Client",
  "scripts": {
    "release": "standard-version -a && git push --follow-tags origin main",
    "clean": "rm -rf node_modules && rm -rf yarn.lock package-lock.json",
    "postinstall": "pip install -r public/src/requirements.txt",
    "testaudio": "clear && node --no-warnings test/audio.js",
    "testvideo": "clear && node --no-warnings test/video.js",
    "update": "npx npm-check-updates -u",
    "post": "npm publish"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "latest",
    "@ffprobe-installer/ffprobe": "latest",
    "@types/fluent-ffmpeg": "latest",
    "chalk": "latest",
    "conventional-github-releaser": "latest",
    "express": "latest",
    "fluent-ffmpeg": "latest",
    "git-authors-cli": "latest",
    "http": "latest",
    "nano-staged": "latest",
    "node-fetch": "latest",
    "npm-check-updates": "latest",
    "openai": "^3.2.1",
    "progress-estimator": "^0.3.0",
    "readline": "^1.3.0",
    "standard": "latest",
    "standard-version": "latest",
    "youtube-dl-exec": "^2.4.0",
    "yt-dlp-exec": "latest"
  },
  "devDependencies": {
    "bin-version-check": "~5.0.0",
    "dargs": "latest",
    "dotenv": "^16.0.3",
    "execa": "latest",
    "is-unix": "latest",
    "spinnies": "latest",
    "winston": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magneum"
  },
  "keywords": [
    "audio",
    "video",
    "youtube",
    "python",
    "javascript",
    "binary",
    "nodejs",
    "yt-dlp",
    "yt-dlp-wrapper",
    "youtube-downloader"
  ],
  "engines": {
    "node": ">= 14"
  },
  "author": {
    "email": "magneumpro@gmail.com",
    "name": "bit.ly/magneum",
    "url": "https://bit.ly/magneum"
  },
  "license": "MIT"
}

{
  "name": "yt-dlp",
  "type": "module",
  "main": "index.js",
  "version": "0.0.2",
  "description": "yt-dlp - Python+ Javascript Youtube Audio Video Scrapper And Downloader Client",
  "scripts": {
    "preinstall": "echo '\n\n[RUNNING PREINSTALL]' && pip install -r src/requirements.txt && npm run update",
    "release": "echo '\n\n[GIT PUBLISH]' && standard-version -a && git push --follow-tags origin main",
    "update": "echo '\n\n[UPDATING DEPENDENCIES]' && npx npm-check-updates -u",
    "clean": "echo '\n\n[REMOVING NODE MODULES]' && rm -rf node_modules",
    "nrelease": "echo '\n\n[NPM PUBLISH]' &&  && npm publish",
    "view": "echo '\n\n[RUNNING DEV]' && node view/index.js"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "latest",
    "@ffprobe-installer/ffprobe": "latest",
    "@types/fluent-ffmpeg": "latest",
    "chalk": "latest",
    "conventional-github-releaser": "latest",
    "fluent-ffmpeg": "latest",
    "git-authors-cli": "latest",
    "nano-staged": "latest",
    "node-fetch": "latest",
    "npm-check-updates": "latest",
    "progress-estimator": "latest",
    "standard": "latest",
    "standard-version": "latest",
    "youtube-dl-exec": "latest",
    "yt-dlp": "latest",
    "yt-dlp-exec": "latest"
  },
  "devDependencies": {
    "bin-version-check": "~5.0.0",
    "dargs": "~8.1.0",
    "dotenv": "latest",
    "execa": "~7.1.1",
    "is-unix": "~2.0.7",
    "spinnies": "latest",
    "winston": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/magneum"
  },
  "keywords": [
    "audio",
    "video",
    "youtube",
    "python",
    "javascript",
    "binary",
    "nodejs",
    "yt-dlp",
    "yt-dlp-wrapper",
    "youtube-downloader"
  ],
  "engines": {
    "node": ">= 14"
  },
  "author": {
    "email": "magneumpro@gmail.com",
    "name": "bit.ly/magneum",
    "url": "https://bit.ly/magneum"
  },
  "license": "MIT"
}